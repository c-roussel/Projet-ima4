<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.thomas.menu.FicheContact">

    <TextView
        android:id="@+id/pseudo"
        android:layout_width="270dp"
        android:layout_height="32dp"
        android:layout_x="-2dp"
        android:layout_y="0dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="15dp" />

    <TextView
        android:id="@+id/num"
        android:layout_width="269dp"
        android:layout_height="75dp"
        android:layout_x="-1dp"
        android:layout_y="55dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="70dp" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/bt2"
        android:layout_width="63dp"
        android:layout_height="56dp"
        android:layout_x="288dp"
        android:layout_y="420dp"
        android:clickable="true"
        app:backgroundTint="@android:color/holo_orange_light"
        app:fabSize="mini"
        app:srcCompat="@android:drawable/ic_delete" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/bt3"
        android:layout_width="85dp"
        android:layout_height="71dp"
        android:layout_x="31dp"
        android:layout_y="401dp"
        android:clickable="true"
        app:fabSize="mini"
        app:srcCompat="@android:drawable/sym_action_call" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/bt1"
        android:layout_width="71dp"
        android:layout_height="69dp"
        android:layout_x="164dp"
        android:layout_y="399dp"
        android:clickable="true"
        app:fabSize="mini"
        app:srcCompat="@android:drawable/stat_notify_chat" />
</AbsoluteLayout>
